datasource db {
  provider = "postgres"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Student {
  id        Int      @id
  number  Int @default(-1)
  displaySemester  Int @default(1)
  librusLogin  String  @default("")
  librusPassword  String  @default("")
  createdAt DateTime @default(now())
}